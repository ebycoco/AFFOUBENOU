<form method="post" action="{{ path('admin_service_web_delete', {'id': service_web.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service_web.id) }}">
    <button class="btn">Delete</button>
</form>
