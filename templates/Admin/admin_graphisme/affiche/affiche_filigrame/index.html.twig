{% extends 'baseAdmin.html.twig' %}

{% block title %}AfficheFiligrame index{% endblock %}

{% block body %}
    <h1>AfficheFiligrame index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>ImageName</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for affiche_filigrame in affiche_filigrames %}
            <tr>
                <td>{{ affiche_filigrame.id }}</td>
                <td>{{ affiche_filigrame.imageName }}</td>
                <td>{{ affiche_filigrame.createdAt ? affiche_filigrame.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ affiche_filigrame.updatedAt ? affiche_filigrame.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('admin_affiche_filigrame_show', {'id': affiche_filigrame.id}) }}">show</a>
                    <a href="{{ path('admin_affiche_filigrame_edit', {'id': affiche_filigrame.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table> 
{% endblock %}
