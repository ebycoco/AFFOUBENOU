{% extends 'baseAdmin.html.twig' %}


{% block title %}MainLogo index{% endblock %}

{% block body %}
    <h1>MainLogo index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>ImageName</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for main_logo in main_logos %}
            <tr>
                <td>{{ main_logo.id }}</td>
                <td>{{ main_logo.imageName }}</td>
                <td>{{ main_logo.createdAt ? main_logo.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ main_logo.updatedAt ? main_logo.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('admin_main_logo_show', {'id': main_logo.id}) }}">show</a>
                    <a href="{{ path('admin_main_logo_edit', {'id': main_logo.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_main_logo_new') }}">Create new</a>
{% endblock %}
